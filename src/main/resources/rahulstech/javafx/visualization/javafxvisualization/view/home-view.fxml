<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rahulstech.javafx.visualization.javafxvisualization.controller.HomeController">
   <children>
      <ToolBar fx:id="toolbar" nodeOrientation="RIGHT_TO_LEFT" prefHeight="56.0">
        <items>
          <Button contentDisplay="RIGHT" mnemonicParsing="false" onAction="#onUploadBackupFile" text="Upload Backup File" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font>
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-primary" />
               </styleClass>
               <graphic>
                  <FontIcon iconColor="WHITE" iconLiteral="fas-upload" />
               </graphic>
            </Button>
        </items>
      </ToolBar>
      <StackPane fx:id="contentPanel" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
